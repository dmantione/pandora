	<entry name="Savanna" value="Savana"/>
	<entry name="SavannaDescription" value="Le savane sono caratterizzate da un clima caldo e secco e, con le loro ampie pianure fertili, sono candidate accettabili per l'agricoltura."/>
	<entry name="SavannaProperties" value="<icon texture='GUI/Bullet'/>Territorio"/>

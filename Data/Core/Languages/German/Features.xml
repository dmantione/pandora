	<entry name="SeafoodBonus0" value="Fische"/>
	<entry name="SeafoodBonus0Description" value="Im Meer in diesem Gebiet sind zahlreiche essbare Lebensformen zu finden, die eine äußerst ergiebige Nahrungsquelle für die umliegenden Städte bilden können."/>
	<entry name="SeafoodBonus0Properties" value="<icon texture='GUI/Bullet'/>Gelände"/>
	<entry name="Storm" value="Ionensturm"/>
	<entry name="StormDescription" value="Vor der Ankunft der Messari werden Gebiete durch massive elektromagnetische Stürme in Mitleidenschaft gezogen, die bei allen, die sich dort aufhalten, schweren Schaden verursachen."/>
	<entry name="StormProperties" value="<icon texture='GUI/Bullet'/>Effekt<br/><icon texture='GUI/Bullet'/>Verursacht 1,0 Schaden pro Runde<br/><icon texture='GUI/Bullet'/>+2 <icon texture='Attributes/Movement'/> Bewegungskosten"/>
	<entry name="Suburb" value="Vorstadt"/>
	<entry name="SuburbDescription" value="Diese überschaubare aber luxuriöse Wohngegend ist ideal geeignet für unsere Arbeiterklasse die sich nach einem harten Tag in den Minen oder Fabriken ins vertraute Heim zur Familie zurückziehen kann."/>
	<entry name="SuburbProperties" value="<icon texture='GUI/Bullet'/>Konstruktion"/>
	<entry name="SuburbStats" value="<icon texture='Resources/Habitat'/> +2 <icon texture='Resources/Credits'/> -3"/>
	<entry name="Tropical" value="Tropisch"/>
	<entry name="TropicalDescription" value="Tropische Zonen zeichnen sich aus durch ein warmes und feuchtes Klima, eine üppige Vegetation sowie einen an Nährstoffen relativ armen Boden."/>
	<entry name="TropicalProperties" value="<icon texture='GUI/Bullet'/>Gelände"/>
